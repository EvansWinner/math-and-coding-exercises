! Calculate the perimeter of a triangle based on given
! length of the two legs
program trianglePerimeter

  real :: a,b,h,p
  write (*,*) "Please input the lengths of the two"
  write (*,*) "legs. Please separate two real numbers"
  write (*,*) "or integers by spaces or newlines"
  read (*,*) a,b

  h = sqrt(a*a+b*b)
  p

end program
